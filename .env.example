# OAuth Configuration
OAUTH_GITHUB_CLIENT_ID=your_github_client_id
OAUTH_GITHUB_CLIENT_SECRET=your_github_client_secret
OAUTH_GOOGLE_CLIENT_ID=your_google_client_id
OAUTH_GOOGLE_CLIENT_SECRET=your_google_client_secret

# LLM Provider API Keys
OPENROUTER_API_KEY=your_openrouter_api_key

# Payment Provider
POLAR_ACCESS_TOKEN=your_polar_access_token

# Development Settings
NODE_ENV=development
GO_ENV=development

# Database (optional for local development - Docker handles this)
DATABASE_URL=postgres://cogent:cogent_dev_password@localhost:5432/cogent?sslmode=disable

# JWT Secret (change in production)
JWT_SECRET=dev_jwt_secret_change_in_production

# Redis (optional for local development - Docker handles this)
REDIS_URL=redis://localhost:6379