{
  "project": {
    "name": "COGENT",
    "id": "cogent-main",
    "path": "/home/il246/Workspace/cogent",
    "language": "go"
  },
  "documentation": {
    "style": {
      "format": "markdown",
      "detail_level": "detailed"
    },
    "templates": {
      "file_header": "default.md",
      "function_doc": "default.md",
      "class_doc": "default.md"
    }
  },
  "patterns": {
    "include": [
      "**/*.go",
      "**/*.js",
      "**/*.ts",
      "**/*.tsx",
      "**/*.py"
    ],
    "exclude": [
      "node_modules/**",
      "vendor/**",
      "*.test.*",
      "*.spec.*",
      "**/.git/**",
      "**/dist/**",
      "**/build/**",
      "**/.cogent/**"
    ]
  },
  "visibility": "private",
  "hooks": {
    "enabled": true,
    "timeout": 30000,
    "tools": ["Edit", "Write", "MultiEdit"]
  },
  "api": {
    "key": "dev-project-key-change-in-production",
    "backend_url": "http://localhost:8080"
  }
}